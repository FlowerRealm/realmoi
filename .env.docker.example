# 可留空；建议改为在管理后台按上游渠道配置
REALMOI_OPENAI_API_KEY=
REALMOI_OPENAI_BASE_URL=https://api.openai.com

REALMOI_JWT_SECRET=change-me
REALMOI_ALLOW_SIGNUP=1
REALMOI_ADMIN_USERNAME=admin
REALMOI_ADMIN_PASSWORD=admin-password-123

